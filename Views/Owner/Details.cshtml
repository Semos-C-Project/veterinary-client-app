@model veterinary_client_app.Models.Entities.Owner

@{
    ViewData["Title"] = "Owner Details";
}

<h2>Details</h2>

<div>
    <h4>Owner</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.OwnerId
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Age
        </dt>
        <dd class="col-sm-10">
            @Model.Age
        </dd>
    </dl>
    <a href="@Url.Action("Edit", "Owner", new { id = Model.OwnerId })">Edit</a> |
    <a href="@Url.Action("Delete", "Owner", new { id = Model.OwnerId })">Delete</a> |
    <a href="@Url.Action("Index")">Back to List</a>
</div>